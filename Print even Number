#include <iostream.h>
#include<conio.h>
using namespace std;


int main() {
 
    int i;
 
    /* Run a loop from 1 to 100. */
 
    for(i = 1; i <= 100; i++){
  
       /* If number is divisible by 2. */
  
      if(i % 2 == 0) {

         cout << i <<"\n"; 
      }
   }
    return 0;
}
