#include <iostream>
using namespace std;

int main() {
 
    int i,number;
 
    cout<<"\nEnter the number : ";
    cin>>number;
    /* Run a loop from 1 to 100. */
 
    for(i = 1; i <= number; i++){
  
       /* If number is divisible by 2. */
  
      if(i % 2 == 0) {

         cout << i <<" "; 
      }
   }
    return 0;
}
